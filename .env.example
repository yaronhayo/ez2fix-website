# Environment Variables Template
# Copy to .env and fill in your values

# Resend API for contact forms (optional)
RESEND_API_KEY=your_resend_api_key_here

# Google Analytics (optional)
PUBLIC_GA_ID=your_google_analytics_id_here

# reCAPTCHA for form protection (optional)
PUBLIC_RECAPTCHA_SITE_KEY=your_recaptcha_site_key_here
RECAPTCHA_SECRET_KEY=your_recaptcha_secret_key_here

# Google Places API for address autocomplete (optional)
PUBLIC_GOOGLE_PLACES_API_KEY=your_google_places_api_key_here