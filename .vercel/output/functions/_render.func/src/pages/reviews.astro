    function showMoreReviews() {
      if (currentBatch >= totalBatches) {
        return; // No more reviews to show
      }
      
      currentBatch++;
      const startIndex = (currentBatch - 1) * reviewsPerBatch;
      const endIndex = startIndex + reviewsPerBatch;
      const reviewsContainer = document.getElementById('reviews-container');
      
      // Get the reviews for this batch from the reviews array
      const reviewsToShow = reviews.slice(startIndex, endIndex);
      
      reviewsToShow.forEach(review => {
        const reviewCard = document.createElement('div');
        reviewCard.className = 'p-6 shadow-xl border-2 h-full';
        reviewCard.style.cssText = 'background-color: #F3F3E6; border-color: #BB8525; border-radius: 0.5rem;';
        
        reviewCard.innerHTML = `
          <div class="text-center">
            <div class="flex justify-center mb-4">
              ${Array(review.rating).fill('<svg class="h-5 w-5" style="color: #D2A63C;" fill="currentColor" viewBox="0 0 24 24"><path d="M12 .587l3.668 7.431 8.332 1.151-6.064 5.828 1.48 8.279-7.416-3.967-7.417 3.967 1.481-8.279-6.064-5.828 8.332-1.151z"/></svg>').join('')}
            </div>
            <blockquote class="text-base leading-relaxed mb-6 text-center italic" style="color: #1D1912;">
              "${review.text}"
            </blockquote>
            <div class="w-12 h-12 rounded-full flex items-center justify-center font-bold text-lg mx-auto mb-3" style="background-color: #D2A63C; color: #F3F3E6;">
              ${review.name.split(' ').map(n => n[0]).join('')}
            </div>
            <div class="font-semibold text-lg" style="color: #1D1912;">${review.name}</div>
            <div class="text-sm mb-2" style="color: #BB8525;">${review.location}</div>
            <div class="text-xs font-medium px-3 py-1 rounded-full mb-2" style="background-color: #EECD5C; color: #1D1912;">
              ${review.service}
            </div>
            <div class="text-xs" style="color: #BB8525;">${review.date}</div>
          </div>
        `;
        
        reviewsContainer.appendChild(reviewCard);
      });
      
      // Update button text or hide if no more reviews
      const button = document.getElementById('view-more-btn');
      if (currentBatch >= totalBatches) {
        button.style.display = 'none';
      }
    }
  </script>
